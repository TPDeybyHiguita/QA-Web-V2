<body onload="loandName();">
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <!-- Menu -->

            <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme menu_1">

                <div class="message">
                    <p class="message_1">Bienvenido</p>
                </div>



                <ul class="menu-inner py-1" id="jsVerificarpermisosMenu">
                    <!-- Dashboard -->



                </ul>


            </aside>


            <!-- / Menu -->
            <!-- Layout container -->
            <div class="layout-page">
                <!-- Navbar -->

                <header class="p-3 mb-3 border-bottom header_div">
                    <div class="container">
                        <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                            <a href="#" class="d-flex align-items-center mb-2 mb-lg-0 text-dark text-decoration-none">
                                <img class="identidad_logo" src="~/Multime/Mesa de trabajo 17 copia 5.png" alt="">
                            </a>

                            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                                <li><a href="#" class="nav-link link-menu">TP home</a></li>
                                <li><a href="#" class="nav-link link-menu">TP ...</a></li>
                                <li><a href="#" class="nav-link link-menu">TP ...</a></li>
                            </ul>

                            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                                <li>
                                    <a href="#" class="nav-link px-2 link-secondary">
                                        <i class="fa-solid fa-envelope icono_5"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" class="nav-link px-2 link-secondary">
                                        <i class="fa-solid fa-comment icono_6"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" class="nav-link px-2 link-secondary">
                                        <i class="fa-solid fa-file icono_5"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" class="nav-link px-2 link-secondary">
                                        <i class="fa-solid fa-circle-exclamation icono_6"></i>
                                    </a>
                                </li>

                                <li>
                                    <a class="nav-link px-2 link-secondary">
                                        <div class="date" id="date">

                                        </div>
                                    </a>
                                </li>

                            </ul>

                            <div class="dateDay" id="jsDateDay"></div>


                            <div class="dropdown user_name nameUser">
                                <a href="#" class="d-block link-dark text-decoration-none dropdown-toggle nameUserText" data-bs-toggle="dropdown" aria-expanded="false" id="nameUserInicial">

                                </a>

                                <ul class="dropdown-menu text-small colum_b nameUserColum">
                                    <li><a class="dropdown-item" href="~/User/UpdateProfile">Editar perfil</a></li>
                                    @*<li><a class="dropdown-item" href="#">Más</a></li>
                                        <li><a class="dropdown-item" href="#">Más</a></li>*@
                                    <li><hr class="dropdown-divider"></li>
                                    <li><a class="dropdown-item" id="jsLoginOff" onclick="closeSession();">Cerrar session</a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </header>

                <!-- / Navbar -->

                <div class="content-wrapper">
                    <!-- Content -->

                    <div class="container-xxl flex-grow-1 container-p-y">

                        <main class="bodybody ">

                            <div class="position-fixed bottom-0 end-0 p-3 cssMensa" style="z-index: 11">
                                <div id="AlertaNotificaciones" class="toast bg-dark" role="alert" aria-live="assertive" aria-atomic="true">
                                    <div class="toast-header">
                                        <i class="fa-solid fa-bell icono_1"></i>

                                        <strong class="me-auto">Notificación</strong>
                                        <small id="jsFecha"></small>
                                        <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                                    </div>
                                    <div class="toast-body" id="jsMensaje">
                                        Loadin...
                                    </div>
                                </div>
                            </div>

                            <!-- Large Modal -->
                            <div class="modal fade" id="modalNewManager" tabindex="-1" aria-hidden="true">
                                <div class="modal-dialog modal-lg" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title" id="exampleModalLabel3">Nuevo manager</h5>
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                        </div>
                                        <div class="modal-body">

                                            <div class="alert alert-dark" id="AlertManager" role="alert" style="display:none;">
                                                Loadin...
                                            </div>

                                            <div class="row g-2">
                                                <div class="col mb-0">
                                                    <label for="emailLarge" class="form-label">CCMS</label>
                                                    <input type="number" id="bitacoraCCMS" class="form-control" onchange="loadInfoManager();">
                                                </div>

                                                <div class="col mb-0" id="bitacoraForm_1" style="display:none;">
                                                    <label for="dobLarge" class="form-label">Usuario de red</label>
                                                    <input type="text" id="bitacoraUsuarioRed" class="form-control" readonly>
                                                </div>
                                            </div>
                                            <div class="row g-2">
                                                <div class="col mb-0" id="bitacoraForm_2" style="display:none;">
                                                    <label for="dobLarge" class="form-label">NOMBRE</label>
                                                    <input type="text" id="bitacoraNombre" class="form-control" readonly>
                                                </div>

                                                <div class="col mb-0" id="bitacoraForm_3" style="display:none;">
                                                    <label for="emailLarge" class="form-label">Correo electrónico</label>
                                                    <input type="email" id="bitacoraCorreoElectronico" class="form-control" readonly>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-label-secondary" data-bs-dismiss="modal">Cerrar</button>
                                            <button type="button" class="btn btn-primary" onclick="newManager();">Agregar</button>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="row_3">
                                <div class="card">
                                    <div class="ManagerCard">
                                        <h5 class="card-header">Bitacora</h5>

                                        <button type="button" class="btn rounded-pill btn-icon btn-secondary" onclick="sendEmailBitacora();">
                                            <i class="fa-solid fa-envelope"></i>
                                        </button>
                                    </div>


                                    <div class="table-responsive text-nowrap">
                                        <table class="table table-hover">
                                            <thead>
                                                <tr>
                                                    <th>ID CASO</th>
                                                    <th>NOMBRE CLIENTE</th>
                                                    <th>MOTIVO</th>
                                                    <th>COMENTARIO</th>
                                                </tr>
                                            </thead>
                                            <tbody class="table-border-bottom-0" id="bitacoraTable">
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="row_3">
                                <div class="card">
                                    <div class="ManagerCard">
                                        <h5 class="card-header">Managers</h5>

                                        <button type="button" class="btn rounded-pill btn-icon btn-secondary" onclick="modalNewManager('modalNewManager');">
                                            <i class="fa-solid fa-plus"></i>
                                        </button>
                                    </div>


                                    <div class="table-responsive text-nowrap">
                                        <table class="table table-hover">
                                            <thead>
                                                <tr>
                                                    <th>CCMS</th>
                                                    <th>Nombre</th>
                                                    <th>Correo</th>
                                                    <th>Estado</th>
                                                    <th>Acciones</th>
                                                </tr>
                                            </thead>
                                            <tbody class="table-border-bottom-0" id="bitacoraManagers">
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="row_3">

                                <div class="card">
                                    <div class="ManagerCard">
                                        <h5 class="card-header">Mensajes Automáticos </h5>
                                    </div>

                                    <div class="formBitacora">
                                        <input class="form-control intupBitacora_1" type="time" value="" id="bitacoraTimeSendEmails" min="01:00" max="20:00">
                                        <button type="button" class="btn rounded-pill btn-icon btn-secondary" onclick="updateTimeSendEmail();">
                                            <i class="fa-solid fa-arrow-right"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>

                </main>

                    </div>
                </div>
                <!-- / Content -->
                <!-- Footer -->
                <footer class="content-footer footer bg-footer-theme">
                    <div class="container-xxl d-flex flex-wrap justify-content-between py-2 flex-md-row flex-column">
                        TP
                    </div>
                </footer>
                <!-- / Footer -->

                <div class="content-backdrop fade"></div>
            </div>
            <!-- Content wrapper -->
        </div>
        <!-- / Layout page -->
    </div>


    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>
    
    @*************JS******************@

    <script src="~/Js/bitacoraSura.js"></script>
